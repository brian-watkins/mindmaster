{
  "name": "spymaster",
  "version": "1.0.0",
  "description": "Break Codes in Elm",
  "main": "app.js",
  "scripts": {
    "postinstall": "npm-run-all -s install:test:elm install:test:integration",
    "install:test:elm": "cd tests && elm-install",
    "install:test:integration": "cd integrationTests && npm install",
    "test": "npm-run-all -s test:elm test:integration",
    "test:integration": "npm-run-all -s build:integrationTest run:test:integration",
    "run:test:integration": "cd integrationTests && npm test",
    "test:elm": "elm-test",
    "dev": "webpack-dev-server --port 3000",
    "build": "webpack",
    "build:integrationTest": "ENVIRONMENT=integrationTest webpack"
  },
  "author": "Brian Watkins",
  "license": "MIT",
  "dependencies": {
    "elm": "^0.18.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "css-loader": "^0.28.11",
    "elm-github-install": "^1.6.1",
    "elm-test": "=0.18.11",
    "elm-webpack-loader": "^4.5.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.9.3",
    "npm-run-all": "^4.1.3",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2"
  }
}
