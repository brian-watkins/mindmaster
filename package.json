{
  "name": "spymaster",
  "version": "1.0.0",
  "description": "Break Codes in Elm",
  "main": "app.js",
  "scripts": {
    "postinstall": "npm-run-all -s install:test:integration",
    "install:test:integration": "cd integrationTests && npm install",
    "test": "npm-run-all -s test:elm test:integration",
    "test:integration": "npm-run-all -s build:integrationTest run:test:integration",
    "run:test:integration": "cd integrationTests && npm test",
    "elm": "ELM_HOME=$(pwd)/node_modules/elmer-test/home elm",
    "test:elm": "ELM_HOME=$(pwd)/node_modules/elmer-test/home elm-test",
    "dev": "ELM_HOME=$(pwd)/node_modules/elmer-test/home webpack-dev-server --port 3000",
    "build": "ELM_HOME=$(pwd)/node_modules/elmer-test/home webpack",
    "build:integrationTest": "ELM_HOME=$(pwd)/node_modules/elmer-test/home ENVIRONMENT=integrationTest webpack"
  },
  "author": "Brian Watkins",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.26.3",
    "elmer-test": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "elm": "^0.19.0-bugfix2",
    "elm-test": "^0.19.0-rev6",
    "elm-webpack-loader": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.3",
    "natives": "^1.1.6",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.2.1"
  }
}
